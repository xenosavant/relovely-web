{
  "name": "relovely",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "docker-start": "ng serve  --host 0.0.0.0",
    "build": "ng build --outputPath=./dist/html",
    "build:circle": "ng build --outputPath=./dist/html",
    "build-prod": "ng build --prod",
    "build-prod:circle": "ng build --prod --outputPath=./dist/html",
    "test": "npm run lint && ng test --configuration=test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "build:prod": "ng build --prod --vendor-chunk",
    "clean": "rimraf ./dist/html",
    "server": "node ./server.js",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "ci": "npm run clean && npm run format:test && ng lint && ng test --configuration=test --browsers ChromeTravisCi --code-coverage && ng e2e && npm run cy:ci && npm run accessibility && npm run build:prod -- --deploy-url /relovely/ --base-href /relovely",
    "release": "standard-version && git push --follow-tags origin master",
    "format:write": "prettier '{src,e2e,cypress}/**/*.{ts,json,md,scss}' --write",
    "format:test": "prettier '{src,e2e,cypress}/**/*.{ts,json,md,scss}' --list-different",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate && node .all-contributors-html.js",
    "contributors:check": "all-contributors check",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "precy:ci": "npm run build:prod -- --deploy-url \"/relovely/\" --base-href \"/relovely\"",
    "cy:ci": "npm-run-all --parallel --race server \"cy:run -- --config=baseUrl=http://localhost:4000\"",
    "preaccessibility": "npm run build:prod -- --deploy-url \"/relovely/\" --base-href \"/relovely\"",
    "accessibility": "npm-run-all --parallel --race server accessibility:run",
    "accessibility:run": "pa11y-ci --threshold 250",
    "compodoc": "compodoc -p src/tsconfig.app.json",
    "docker:prod": "docker-compose -f Production.docker-compose.yml build",
    "docker:prod-up": "docker-compose -f Production.docker-compose.yml up",
    "docker:prod-down": "docker-compose -f Production.docker-compose.yml down"
  },
  "husky": {},
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{src,e2e,cypress}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.1002.3",
    "@angular/animations": "^10.2.5",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@cloudinary/angular-5.x": "^1.4.1",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@sentry/angular": "^6.3.5",
    "@sentry/tracing": "^6.3.5",
    "@stripe/stripe-js": "^1.14.0",
    "browser-detect": "^0.2.28",
    "cloudinary-core": "^2.11.4",
    "cloudinary-video-player": "^1.3.4",
    "core-js": "^3.4.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.27.0",
    "ng-click-outside": "^6.0.0",
    "ng2-dragula": "^2.1.1",
    "ng2-file-upload": "^1.4.0",
    "ngx-image-cropper": "3.1.4",
    "ngx-owl-carousel-o": "4.0.0",
    "ngx-stripe": "10.0.0",
    "rxjs": "^6.5.3",
    "uuid": "^3.3.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.3.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.8",
    "all-contributors-cli": "^6.11.0",
    "cypress": "^3.6.1",
    "express": "^4.17.1",
    "husky": "^2.7.0",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "~0.0.32",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^2.3.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1",
    "standard-version": "6.0.1",
    "ts-node": "~8.2.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.7",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
